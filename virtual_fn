#include <iostream>
using namespace std;

class Shape {
public:
    virtual void draw() = 0; // pure virtual
};

class Circle : public Shape {
public:
    void draw() { cout << "Drawing Circle\n"; }
};

class Rectangle : public Shape {
public:
    void draw() { cout << "Drawing Rectangle\n"; }
};

int main() {
    Shape *s1 = new Circle();
    Shape *s2 = new Rectangle();
    s1->draw();
    s2->draw();
    return 0;
}
