#include<iostream>
#include<string>
using namespace std;
class teacher{
    
public:
 //non_parameterized
teacher(){
        dept="computer science";
    }
    //parameterized
    teacher (string n,string s,string d,double sal){
        name=n;
        subject=s;
        dept=d;
        salary=sal;
    }
string name;
string subject;
string dept;
double salary;
void changedept(string newdept){
    dept=newdept;
}
};
int main(){
    teacher t1("geet","c++","cs",3000);
    cout << t1.dept << endl;
}
