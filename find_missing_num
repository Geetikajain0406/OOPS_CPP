CODE-1:
#include <iostream>
#include<algorithm>
using namespace std;

int main() {
   int arr[4]={1,2,4,5};
   int n=5;
   for(int i=1;i<=n;i++){
       bool isfound=false;
       for(int j=0;j<n;j++){
           if(arr[j]==i){
               isfound = true;
               break;
           }
       }
        if (!isfound) {
            cout << i << " ";
        }
   }
    return 0;
}
CODE-2:
#include <iostream>
#include<algorithm>
using namespace std;

int main() {
   int arr[4]={1,2,4,5};
   int n=5;
   int total=n*(n+1)/2;
   int sum=0;
   for(int i=0;i<4;i++){
       sum+=arr[i];
   }
   int result=total-sum;
   cout<<"the missing number is:"<<result<<endl;
    return 0;
}
