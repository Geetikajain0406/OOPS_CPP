#include<iostream>
#include<string>
using namespace std;
class second{
int pid;
public:

    second (int id){
    pid=id;
    cout<<"the parametrized person id is:"<<pid<<endl;}
    second(){
        pid=0;
    cout<<"the default constructor person id is :"<<pid<<endl;}


};
int main(){
   second s(123);
second s1;

return 0;
}
