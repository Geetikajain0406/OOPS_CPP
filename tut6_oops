#include <iostream>
using namespace std;
    void swap(int &a,int &b){
        int temp=a;
        a=b;
        b=temp;
        cout<<"the ans is:"<<a<<","<<b<<endl;
    }
    void swap(float &x,float &y){
        float temp=x;
        x=y;
        y=temp;
        cout<<"the ans is:"<<x<<","<<y<<endl;
    }
int main() {
    int a=2,b=1;
 swap(a,b);
float x=2.0,y=8.0;
 swap(x,y);
    return 0;
}
Ans-2
#include <iostream>
using namespace std;
    class a{
        int x,y,z;
        public:
        a(){
            
        }
        a(int b,int c,int d){
            x=b;
            y=c;
            z=d;
        }
        a operator -(){
           a res;
           res.x=-x;
           res.y=-y;
           res.z=-z;
           return res;
        }
        void display(){
            cout<<x<<y<<z;
        }
    };
int main() {
    a a1(1,2,3);
    a b1=-a1;
    b1.display();
    return 0;
}
