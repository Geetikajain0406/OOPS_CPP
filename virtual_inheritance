#include <iostream>
using namespace std;
class a{
   int x;
   public:
   a(){
       cout<<"a default constructor "<<endl;
   }
};
class b:virtual public a{
   public:
   b(){
       cout<<"b default constructor "<<endl;
   }
};
class c: virtual public a,public b{
   public:
   c(){
       cout<<"c default constructor "<<endl;
   }
};

int main() {
   c c1;

    return 0;
}
