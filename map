#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

int main() {
    vector<int> a, b;
    int n, x;

    cout << "How many elements for array 1? ";
    cin >> n;

    cout << "Enter " << n << " elements:\n";
    for (int i = 0; i < n; i++) {
        cin >> x;
        a.push_back(x);
    }

    cout << "How many elements for array 2? ";
    cin >> n;

    cout << "Enter " << n << " elements:\n";
    for (int i = 0; i < n; i++) {
        cin >> x;
        b.push_back(x);
    }

    // Insert only non-present elements
    for (int i = 0; i < b.size(); i++) {
        bool found = false;

        for (int j = 0; j < a.size(); j++) {
            if (a[j] == b[i]) {
                found = true;
                break;
            }
        }

        if (!found)
            a.push_back(b[i]);
    }

    sort(a.begin(), a.end());

    cout << "Final array: ";
    for (int i = 0; i < a.size(); i++)
        cout << a[i] << " ";

    return 0;
}
