#include <iostream>
using namespace std;
int main() {
    int count=0;
    int arr[5]={1,2,3,3,4};
    int n=3;
    for (int i=0;i<5;i++ ){
        if(arr[i]==n){
            cout<<i<<endl;
            count++;
        }
    }
cout<<"the total number time ur number appeared is:"<<count<<endl;
    return 0;
}
