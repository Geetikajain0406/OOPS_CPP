#include <iostream>
using namespace std;
int main() {
   int r,c;
   cout<<"enter row & coloumn"<<endl;
   cin>>r>>c;
   int arr[r][c];
   cout<<"enter values for array "<<endl;
   for(int i=0;i<r;i++){
       for(int j=0;j<c;j++){
           cin>> arr[i][j];
       }
   }
   int (*ptr)[c]=arr;
    for(int i=0;i<r;i++){
       for(int j=0;j<c;j++){
           cout<<*(*(ptr+i)+j)<<"";
       }
       cout<<endl;
    }
    return 0;
}
