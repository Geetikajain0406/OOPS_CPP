#include <iostream>
#include<algorithm>
using namespace std;
class A{
    int a,b;
    public:
    A(int x,int y){
        a=x;
        b=y;
    }
    A(A &ref ){
        a=ref.a;
        b=ref.b;
    }
    void show(){
        cout<<"the value ofa and b are:"<<a<<b;
    }
};
int main() {
    A o(1,2);
    A b=o;
    b.show();
    return 0;
}
