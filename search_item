#include <iostream>
using namespace std;
int search(int arr[],int n){
    bool found=0;
   int high=arr[4];
    int low=0;
    while(low<=high){
       int mid=low+high/2;
        if(arr[mid]==n){
            return mid;
            found=1;
        }
        else if(arr[mid]<n){
            low=mid+1;
        }
        else{
            high=mid-1;
        }
    }
    return -1;
}
int main() {
    int count=0;
    int arr[5]={1,2,3,3,4};
    int n=3;
    int s=search(arr,n);
    cout<<s<<endl;
    return 0;
}
