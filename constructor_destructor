#include<iostream>
using namespace std;
class student{
    public:
    string name;
    int age;
    //student(){
        //  name = "unknown";
        //  age=0;
       //   cout<<"default constructor called"<<endl;
   // }
    student(string n,int x){
        name=n;
        age=x;
        cout<<"constructor called student "<<name<<"the age is"<<age<<endl;
    }
    ~student(){
        cout<<"destructor is called here for name"<<name<<endl;
    }
};
int main(){
     // student s1;
    student s2("geetika",18);
    return 0;
}
