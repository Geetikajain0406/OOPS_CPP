#include<iostream>
using  namespace std;
template <class t>
t max1(t vector1[],int s){
    t high=0;
for(int i=0;i<s;i++){
    if(vector1[i]>high){
        high=vector1[i];
    }
}
return high;
}
int main(){
    int vec[5]={1,2,13,4,5};
    int n=5;
cout<<"the max number is : "<<max1<int>(vec,5)<<endl;
return 0;}

